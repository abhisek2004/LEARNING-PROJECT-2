<!DOCTYPE html>
<html lang="en">

    <head>
        <meta charset="UTF-8">
        <meta name="viewport" content="width=device-width, initial-scale=1.0">
        <title>ADMIN DASHBOARD</title>
        <style>
            body {
                font-family: Arial, sans-serif;
                margin: 0;
                padding: 0;
            }

            h1 {
                text-align: center;
                margin-top: 20px;
            }

            table {
                width: 80%;
                margin: 20px auto;
                border-collapse: collapse;
            }

            th,
            td {
                border: 1px solid #dddddd;
                text-align: left;
                padding: 8px;
            }

            th {
                background-color: #f2f2f2;
            }

            tr:nth-child(even) {
                background-color: #f2f2f2;
            }
        </style>
    </head>

    <body>
        <h1>Dine &
            Dash ADMIN DASHBOARD</h1>
        <table>
            <tr>
                <th>First Name</th>
                <th>Last Name</th>
                <th>Email</th>
                <th>Table Type</th>
                <th>Guest Number</th>
                <th>Time</th>
                <th>Date</th>
            </tr>
            <% orderData.forEach(function(orderData) { %>
                <tr>
                    <td><%= orderData.firstName %></td>
                    <td><%= orderData.lastName %></td>
                    <td><%= orderData.email %></td>
                    <td><%= orderData.tableType %></td>
                    <td><%= orderData.guestNum %></td>
                    <td><%= orderData.time %></td>
                    <td><%= orderData.date %></td>
                </tr>
            <% }); %>
        </table>
    </body>

</html>